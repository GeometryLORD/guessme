import random
random_number=random.randint(1, 10)
turns=0
while turns<=5:
  guess=raw_input("Guess a number from 1 to 10! ")
  if guess.isnumeric and len(guess)>0:
    if guess==str(random_number):
      print "You win!"
      break
    else:
      print "Try again!"
      turns+=1
    if turns==5:
      print "You lose!"
      break
  else:
    print "Try again!"
    turns+=1
